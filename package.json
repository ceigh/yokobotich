{
  "name": "yokobotich",
  "version": "1.5.0",
  "dependencies": {
    "better-queue": "^3.8.10",
    "better-queue-memory": "^1.0.4",
    "browser-or-node": "^1.2.1",
    "crypto-js": "^3.1.9-1",
    "jsonp": "^0.2.1",
    "simple-crypto-js": "^2.2.0",
    "tmi.js": "^1.5.0"
  },
  "description": "Skip notifications for twitch chat",
  "main": "src/lib/index.js",
  "bin": "src/node/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint . --fix",
    "test": "node_modules/.bin/jasmine",
    "start": "node src/node/index.js",
    "browser": "./node_modules/.bin/webpack-dev-server",
    "browser:build": "DEBUG=false ./node_modules/.bin/webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ceigh/yokobotich.git"
  },
  "keywords": [
    "twitch",
    "chat",
    "bot"
  ],
  "author": "Artjom LÃ¶bsack",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/ceigh/yokobotich/issues"
  },
  "homepage": "https://gitlab.com/ceigh/yokobotich#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "eslintConfig": {
    "globals": {
      "CFG_HASH": "readonly",
      "DEBUG": "readonly"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "rules": {
      "no-console": 0,
      "import/extensions": 0,
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ],
      "no-return-assign": 0,
      "no-underscore-dangle": 0
    }
  },
  "eslintIgnore": [
    "public"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
